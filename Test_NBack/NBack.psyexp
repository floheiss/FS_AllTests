        <Param val="#stores the currtentrail number&amp;#10;#gets save in the data for later usage &amp;#10;trailNumber = 0" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="trailNumber = 0;&amp;#10;" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="#Holds the NBackOrder&amp;#10;NBackOrder = []&amp;#10;&amp;#10;#What lvl of Nback currently used 1-anything &amp;#10;NBackNumber = 2&amp;#10;&amp;#10;#Holds the number of loops in the nBack&amp;#10;#Is used to stop the loop &amp;#10;NBackLoops = 0&amp;#10;&amp;#10;#holds the correct response for the keyinput &amp;#10;correctResponceNBack = &quot;space&quot;&amp;#10;&amp;#10;debug = 0 " valType="extendedCode" updates="constant" name="Begin Experiment"/>
