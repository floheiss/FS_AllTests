        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="NBackLoops = (NBackLoops + 1);&amp;#10;trailNumber = (trailNumber + 1);&amp;#10;psychoJS.experiment.addData(&quot;trailNumberNBack&quot;, trailNumber);&amp;#10;psychoJS.experiment.addData(&quot;nBackLoop&quot;, NBackLoops);&amp;#10;if ((NBackLoops &gt;= 10)) {&amp;#10;    NBack.finished = true;&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="NBackLoops = NBackLoops + 1&amp;#10;trailNumber = trailNumber  + 1&amp;#10;thisExp.addData('trailNumberNBack', trailNumber)&amp;#10;thisExp.addData('nBackLoop', NBackLoops)&amp;#10;&amp;#10;if(NBackLoops  &gt;= 10): &amp;#10;    NBack.finished = True&amp;#10;" valType="extendedCode" updates="constant" name="End Routine"/>
